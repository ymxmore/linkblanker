{
  "name": "linkblanker",
  "description": "Open link in new tab.",
  "version": "3.0.0",
  "author": "X-MORE LTD",
  "bugs": {
    "url": "https://github.com/ymxmore/linkblanker/issues"
  },
  "devDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.7.0",
    "@types/jest": "^27.5.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/webextension-polyfill": "^0.8.3",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "async-lock": "^1.3.1",
    "autoprefixer": "^10.4.7",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.4.13",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.3.0",
    "tsyringe": "^4.6.0",
    "typescript": "^4.6.4",
    "webextension-polyfill": "^0.9.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "homepage": "https://github.com/ymxmore/linkblanker#readme",
  "keywords": [
    "browser",
    "link",
    "tab"
  ],
  "license": "MIT",
  "main": "src/ts/app/background.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymxmore/linkblanker.git"
  },
  "scripts": {
    "archive": "cross-env NODE_ENV=production npm-run-all build zip",
    "build": "npm-run-all clean cp compile",
    "clean": "npm-run-all --parallel clean:*",
    "clean:dist": "rimraf ./dist/*",
    "compile": "npm-run-all --parallel compile:*",
    "compile:ts": "webpack",
    "cp": "npm-run-all --parallel cp:*",
    "cp:img": "cpx 'src/img/**/*' 'dist/img'",
    "cp:locale": "cpx 'src/_locales/**/*' 'dist/_locales'",
    "cp:manifest": "cpx 'src/manifest.json' 'dist'",
    "lint": "eslint src/ts",
    "lint:fix": "eslint --fix './src/**/*.{js,jsx,ts,tsx}' && prettier --write './src/**/*.{js,jsx,ts,tsx}'",
    "start": "npm-run-all clean cp watch",
    "test": "npm run lint && jest",
    "watch": "npm-run-all --parallel watch:*",
    "watch:img": "onchange -i 'src/img/**/*' -- npm run cp:img",
    "watch:locale": "onchange -i 'src/_locales/**/*' -- npm run cp:locale",
    "watch:manifest": "onchange -i 'src/manifest.json' -- npm run cp:manifest",
    "watch:ts": "webpack -w",
    "zip": "node zip.js"
  }
}
